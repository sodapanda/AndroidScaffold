<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="scaffold_clipview">
        <attr name="corner_radius" format="dimension" />

    </declare-styleable>

    <declare-styleable name="CapsuleView">
        <attr name="cap_color" format="color" />
        <attr name="cap_fill" format="boolean" />
        <attr name="cap_line_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="WrapMaxHeightLayout">
        <attr name="max_height" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ShadowView">
        <attr name="shadow_color" format="color" />
        <attr name="shadow_mask" format="reference" />
    </declare-styleable>

    <declare-styleable name="ColorView">
        <attr name="color" format="color" />
    </declare-styleable>

    <declare-styleable name="RoundedCornerLayout">
        <attr name="radios" format="dimension" />
        <attr name="round_color" format="color" />
    </declare-styleable>

    <declare-styleable name="BubbleTipsView">
        <attr name="bubble_text" format="string" />
    </declare-styleable>
</resources>